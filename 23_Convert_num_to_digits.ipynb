   "source": [
    "# 23.Number of digits in an integer in Python\n",
    "\n",
    "n=str(input(\"Enter the num(only integer):\"))\n",
    "sum=0\n",
    "for i in n:\n",
    "    r=int(i)\n",
    "    for j in range(0,11):\n",
    "        if j==r:\n",
    "            sum=sum+1\n",
    "print(sum)"
   ]
  }

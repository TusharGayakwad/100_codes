   "source": [
    "#Check if a Year is a Leap Year or Not in Python.\n",
    "\n",
    "year=int(input(\"Enter the year:\"))\n",
    "\n",
    "if year%4==0 and year%400 and year%100!=0:\n",
    "    print(f\"{year} is a leep Year.\")\n",
    "else :\n",
    "    print(f\"{year} is not leep Year.\")\n"
   ]
  }

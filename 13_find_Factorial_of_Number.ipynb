   "source": [
    "# 13.Factorial of a Number in Python\n",
    "\n",
    "n=int(input(\"Enter the num:\"))\n",
    "fact=1\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    fact=fact*i\n",
    "print(f\"Factorial of {n}: \",fact)"
   ]
  }

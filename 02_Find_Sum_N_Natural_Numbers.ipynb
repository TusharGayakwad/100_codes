   "source": [
    "#Find the Sum of The First N Natural Numbers in Python\n",
    "\n",
    "x=int(input(\"Enter the num:\"))\n",
    "n=0\n",
    "\n",
    "for i in range(0,x+1):\n",
    "    n=n+i\n",
    "print(n)"

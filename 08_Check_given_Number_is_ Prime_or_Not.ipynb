   "source": [
    "# Check Whether a Number is a Prime or Not in Python\n",
    "\n",
    "x=int(input(\"Enter the value X:\"))\n",
    "flag=0\n",
    "\n",
    "for i in range(1,x+1):\n",
    "    if x%i==0:\n",
    "        flag=flag+1\n",
    "if flag==2:\n",
    "    print(f\"{x} is a Prime num.\")\n",
    "else:\n",
    "    print(f\"{x} is not a Prime num.\")\n",
    "\n"
   ]
  }

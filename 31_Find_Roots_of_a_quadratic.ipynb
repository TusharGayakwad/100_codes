   "source": [
    "# 31.Roots of a quadratic equation in Python\n",
    "\n",
    "import math\n",
    "a,b,c=1,4,4\n",
    "\n",
    "d=math.sqrt((b*b)-(4*a*c))\n",
    "e=2*a\n",
    "\n",
    "root1=((b+d)/e)\n",
    "root2=((b-d)/e)\n",
    "print(\"root first:\",root1,\"and root secend: \",root2)\n"
   ]
  }
